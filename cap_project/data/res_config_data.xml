<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Pour que les ir.attachements des documents soient aussi des documents.documents -->
        <record id="cap_project_config_settings" model="res.config.settings">
            <field name="documents_project_settings">True</field>
        </record>
        <function model="res.config.settings" name="execute">
            <value model="res.config.settings"
                   search="[('id', '=', ref('cap_project_config_settings'))]"/>
        </function>
    </data>
</odoo>